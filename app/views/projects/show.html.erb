<h2><%= @project.name %></h2>

<div id="images">
	<% @project.images.each do |image| %>
		<%= image_tag("content/projects/large/#{image.image_url}", :alt => image.image_url )%>
	<% end %>
</div>

<% @project.people.each do |person| %>
<h1><%= person.id %></h1>
<% end %>

<h4>See more projects in:<br />
	<ul>
		<% @project.categories.each do |category| %>
		<li><%= link_to category.name, projects_path(:anchor => category.name.downcase) %></li>
		<% end %>
	</ul>
</h4>