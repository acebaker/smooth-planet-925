<h2 id="brand">Brand Design</h2>
	<% @projects.each_with_index do |project, index| %>
		<% iteration = index%3 %>
  		<% if iteration == 2 %>
			<%= "<div class='work'>#{image_tag("content/circles/Circles_0#{Circle.random.id}.gif")}</div>".html_safe %>
		<% end %>
		<%= render project %>
	<% end %>

