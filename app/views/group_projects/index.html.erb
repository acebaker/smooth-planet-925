<div id="all" class="category">
	<% @projects.each do |project| %>
	<div id="<%= project.name.downcase.gsub(/ /,'_') %>" class="category">
		<h2><%= project.name %></h2>
		<p><%= project.description %></p>
		<ul>
		<% project.people.each do |p| %>
			<li><%= link_to p.fname + p.lname, "/people/#{p.id}" %></li>
		<% end %>
		</ul>
	</div>
	<% end %>
	
	<div class="clear"></div>
</div> <!-- end #all -->


