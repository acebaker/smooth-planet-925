<div id="all" class="category">
	<% @projects.each do |project| %>
	<div class="work">
		<h2><%= project.name %></h2>
		<p><%= project.description %></p>
		<ul>
		<% project.people.each do |person| %>
			<li><%= person.fname + person.lname %></li>
		<% end %>
		</ul>
	</div>
	<% end %>
	
	<div class="clear"></div>
</div> <!-- end #all -->